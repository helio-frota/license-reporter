{
  "name": "license-reporter",
  "version": "1.5.0",
  "description": "License-reporter gathers license information and reports them in various formats.",
  "main": "dist/index.js",
  "scripts": {
    "build-dist": "tsc",
    "copy-files": "cp -r ./lib/utils/resources/ ./dist/lib/utils/resources",
    "pretest": "eslint . --ext .ts",
    "test": "npm run build-dist && npm run copy-files && node --test dist/test/lib/dual.test.js dist/test/lib/project.test.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": "18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeshift/license-reporter.git"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "js-yaml": "^4.1.0",
    "js2xmlparser": "^4.0.1",
    "jsonschema": "^1.2.9",
    "license-checker": "^25.0.1",
    "mustache": "^4.2.0",
    "node-project-validator": "^0.1.3",
    "xml2js": "^0.4.23",
    "yargs": "^17.1.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^18.7.20",
    "@types/recursive-readdir": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "proxyquire": "^2.1.3",
    "rewire": "^5.0.0"
  },
  "preferGlobal": true,
  "bin": {
    "license-reporter": "bin/license-reporter"
  },
  "support": true
}
